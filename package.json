{
  "name": "frontend",
  "engines": {
    "node": ">=12.0"
  },
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "mocha",
    "dev": "vite",
    "build": "npm run prebuild && vite build",
