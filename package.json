    "stylelint": "npx stylelint \"**/*.scss\"",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/chai": "^4.3.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.5.7",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "chai": "^4.3.10",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "mocha": "^10.3.0",
    "nanoid": "^4.0.2",
    "sass": "^1.64.2",
    "sinon": "^16.0.0",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-handlebars": "^1.6.0"
  },
  "husky": {
    "hooks": {
